{
  "name": "myNEU autoLogin",
  "version": "1.0.1",
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://myneu.neu.edu/cp/home/displaylogin"
      ],
      "js": ["login.js"]
    }
  ],
  "browser_action": {
    "default_title": "muNEU-autoLogin",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
